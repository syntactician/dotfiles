super + Escape
	pkill -USR1 -x sxhkd

super + Return
	st $SHELL -l

super + shift + Return
	urxvt

super + !button1
	wmv -a %i %i $(pfw)

super + {h, j, k, l}
	~/.wm/snap.sh {west, south, north, east} $(pfw)

super + f
	~/.wm/snap.sh full $(pfw)

super + shift + c
	~/.wm/snap.sh center $(pfw)

super + shift + {h, l}
	~/.wm/snap.sh {northwest, northeast} $(pfw)

super + alt + {h, l}
	~/.wm/snap.sh {southwest, southeast} $(pfw)

super + {Tab, grave}
	~/.wm/focus.sh {next, prev}

super + space
	dmenu_run

super + shift + e
	pkill xinit

super + c
	killw $(pfw)

super + shift + f
	firefox

super + shift + return
	xterm

super + s
	~/.wm/scratch.sh

super + p
	spt -n 'notify-send'

super + BackSpace
	slock

super + shift + BackSpace
	poweroff
